{
  "name": "mongo-portable",
  "version": "1.4.0",
  "description": "Portable Pure JS MongoDB - Based on Monglodb (https://github.com/euforic/monglodb.git) by Christian Sullivan (http://RogueSynaptics.com)",
  "author": "Eduardo Astolfi <eastolfi91@gmail.com>",
  "keywords": [
    "mongo",
    "portable",
    "ddbb",
    "no",
    "sql",
    "relational",
    "store",
    "node"
  ],
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "nyc": {
    "include": [
      "src/**/*.{js,ts,tsx}"
    ],
    "exclude": [
      "**/node_modules/**",
      "src/**/*.d.ts",
	  "src/**/index.ts",
	  "index.ts"
    ],
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "produceSourceMap": true,
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "dependencies": {
    "jsw-logger": "^1.2.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^7.0.4",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "conventional-github-releaser": "^1.1.3",
    "coveralls": "^2.11.9",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.5.1",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-coveralls": "^0.1.4",
    "gulp-gh-pages": "^0.5.4",
    "gulp-git": "^1.12.0",
    "gulp-jsdoc-to-markdown": "^1.2.2",
    "gulp-minify": "0.0.14",
    "gulp-mocha": "^3.0.1",
    "gulp-mocha-phantomjs": "^0.12.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.1.4",
    "gulp-util": "^3.0.8",
    "jscoverage": "^0.6.0",
    "jsdoc-to-markdown": "^2.0.1",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "10.1.2",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.3.0",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "test.old": "gulp test",
    "test": "mocha test/unit/utils/*.spec.ts",
    "test.coverage": "nyc mocha test/unit/utils/*.spec.ts",
    "build": "tsc -p ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EastolfiWebDev/MongoPortable.git"
  },
  "homepage": "https://github.com/EastolfiWebDev/MongoPortable#readme",
  "bugs": {
    "url": "https://github.com/EastolfiWebDev/MongoPortable/issues"
  },
  "license": "MIT"
}
