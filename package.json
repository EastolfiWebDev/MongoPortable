  "devDependencies": {
    "babel-preset-es2015": "6.6.0",
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "grunt": "1.0.1",
    "grunt-babel": "6.0.0",
    "grunt-cli": "1.2.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-coveralls": "1.0.1",
    "grunt-jsdoc": "2.0.0",
    "grunt-jsdoc-to-markdown": "1.2.1",
    "grunt-simple-mocha": "0.4.1",
    "jscoverage": "0.6.0",
    "mocha": "2.4.5",
    "mocha-lcov-reporter": "1.2.0"
  },
